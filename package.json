{
  "dependencies": {
    "bootstrap": "^5.2.3",
    "copy-files-from-to": "^3.6.0"
  },
  "scripts": {
    "postinstall": "copy-files-from-to"
  },
  "copyFiles": [
    {
      "from": "node_modules/bootstrap/dist/css/bootstrap.css",
      "to": "Web/Content/bootstrap.css"
    },
    {
      "from": "node_modules/bootstrap/dist/css/bootstrap.css.map",
      "to": "Web/Content/bootstrap.css.map"
    },
    {
      "from": "node_modules/bootstrap/dist/js/bootstrap.bundle.js",
      "to": "Web/Scripts/bootstrap.js"
    },
    {
      "from": "node_modules/bootstrap/dist/js/bootstrap.bundle.js.map",
      "to": "Web/Scripts/bootstrap.bundle.js.map"
    }
  ],
  "copyFilesSettings": {
    "whenFileExists": "overwrite"
  }
}
